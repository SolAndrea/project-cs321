#:import utils kivy.utils

ScreenManagement:
    MainScreen:
        id: "main"
    Guide:
        id: "guide"
	BananasCla:
        id: 'bananas'
	TomatoesCla:
        id: 'tomatoes'
	AvocadosCla:
    	id: 'avocados'
	StrawberrysCla:
		id: "strawberry"
	Classifier:
        id: "classifier"

<MainScreen>:
    name: 'main'
	GridLayout:
        cols:1
        size: root.width, root.height

        #to make the background white
        canvas.before:
            Rectangle:
                pos: self.pos
                size: self.size

        BoxLayout:
            #overall layout of main page
            spacing: 20
            pos_hint: {'y': 0.5}
            orientation: 'vertical'

            #adds the top bar
            Label:
                text: "[b]Main CS321 Project:[/b] \nFruit Ripeness Classifier and Guide"
                markup: True
                canvas.before:
                    Color:
                        rgba: utils.get_color_from_hex('#c70039')
                    Rectangle:
                        pos: self.pos
                        size: self.size
                font_name: './program/uiElements/coolvetica.ttf'
				font_size: 20
                size_hint_y: None
                height: 55
                halign:"center"

            #spacing for the elements on the page
            Widget:
            Button:
				on_release: app.root.current = 'classifier'
                background_normal: './program/uiElements/genericButton.PNG'
                background_down: './program/uiELements/genericButton_pressed.PNG'
				text: '[b]Classify an image[/b]'
                color: utils.get_color_from_hex('#2d862d')
                markup: True
                font_name: './program/uiElements/coolvetica.ttf'
				font_size: 30
                pos_hint: {'center_x': .5}
                size: (400, 100)
                size_hint: (None, None)


            #spacing for the elements on the page
            Widget:
			Button:
				on_release: app.root.current = 'guide'
                background_normal: './program/uiElements/genericButton.PNG'
                background_down: './program/uiELements/genericButton_pressed.PNG'
				text: '[b]Infographic[/b]'
                color: utils.get_color_from_hex('#ff0000')
                markup: True
                font_name: './program/uiElements/coolvetica.ttf'
				font_size: 30
                pos_hint: {'center_x': .5}
                size: (400, 100)
                size_hint: (None, None)

            #spacing for the elements on the page
            Widget:

            #adds the bottom bar
            Label:
                canvas.before:
                    Color:
                        rgba: utils.get_color_from_hex('#c70039')
                    Rectangle:
                        pos: self.pos
                        size: self.size
                size_hint_y: None
                height: 55




<Guide>:
